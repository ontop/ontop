<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">

<urlrewrite use-context="true" use-query-string="true">

	<rule>
		<from>^(.*)/$</from>
		<to type="redirect">$1/overview.view</to>
	</rule>

</urlrewrite>