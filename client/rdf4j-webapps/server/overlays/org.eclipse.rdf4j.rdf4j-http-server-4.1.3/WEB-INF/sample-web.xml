<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee web-app_2_4.xsd" version="2.4">
	<display-name>Common webapp base</display-name>
	<description>Common webapp base</description>
	<filter>
		<filter-name>PathFilter</filter-name>
		<filter-class>org.eclipse.rdf4j.common.webapp.filters.PathFilter</filter-class>
	</filter>
	<filter>
		<filter-name>UrlRewriteFilter</filter-name>
		<filter-class>org.tuckey.web.filters.urlrewrite.UrlRewriteFilter</filter-class>
		<init-param>
			<param-name>logLevel</param-name>
			<param-value>commons</param-value>
		</init-param>
		<init-param>
			<param-name>statusEnabled</param-name>
			<param-value>false</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>UrlRewriteFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>PathFilter</filter-name>
		<url-pattern>*.css</url-pattern>
	</filter-mapping>
	<servlet>
		<servlet-name>common-webapp-base</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
		<load-on-startup>100</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>common-webapp</servlet-name>
		<url-pattern>*.view</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>common-webapp-base</servlet-name>
		<url-pattern>*.form</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>overview.view</welcome-file>
	</welcome-file-list>
</web-app>
