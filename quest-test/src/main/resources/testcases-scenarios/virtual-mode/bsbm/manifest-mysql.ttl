@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix : <http://obda.org/quest/tests/bsbm#> .
@prefix rdfs:	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix mf:     <http://obda.org/quest/tests/test-manifest#> .
@prefix qt:     <http://obda.org/quest/tests/test-query#> .
@prefix obdat:   <http://obda.org/quest/tests/test-scenario#> .

<>  rdf:type mf:Manifest ;
    rdfs:label "BSBM Queries" ;
    mf:entries
    ( 
      :query-1
      :query-2
      :query-3
      :query-4
      :query-5  # original query
      :query-5b # with modification
      :query-6
      :query-7
      :query-8
      :query-10  # original query
      :query-10b # with modification, removing casting call xsd:double(str(?price)) in ORDER BY
      :query-11

      #:query-9 NOT EXIST! The original test uses DESCRIBE
      #:query-12 NOT EXIST! The original test uses CONSTRUCT
    ) .

:query-1 rdf:type mf:QueryEvaluationTest ;
    mf:name    "Q1: Find products for a given set of generic features" ;
    obdat:approval obdat:Approved ;
    obdat:approvedBy <http://wifo5-03.informatik.uni-mannheim.de/bizer/berlinsparqlbenchmark/spec/20080912/index.html#queryTripleQ1> ;
    mf:action
            [ qt:query <query-1.rq> ] ;
    mf:result <query-result-1.ttl> ;
    mf:knowledgebase <bsbm.owl> ;
    mf:mappings <bsbm-mysql.obda> .

