[PrefixDeclaration]
:		http://obda.inf.unibz.it/ontop/benchmark/bsbm.owl#
dc:		http://purl.org/dc/elements/1.1/
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
rev:		http://purl.org/stuff/rev#
bsbm:		http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/vocabulary/
foaf:		http://xmlns.com/foaf/0.1/
rdfs:		http://www.w3.org/2000/01/rdf-schema#
iso3166:		http://downlode.org/rdf/iso-3166/countries#
bsbm-inst:	http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/

[SourceDeclaration]
sourceUri	BSBM-Benchmark
connectionUrl	jdbc:mysql://10.7.20.39/bsbm250k
username	fish
password	fish
driverClass	com.mysql.jdbc.Driver

[MappingDeclaration] @collection [[
mappingId	Offer
target		bsbm-inst:dataFromVendor{publisher}/Offer{nr} a bsbm:Offer ; bsbm:price {price} ; bsbm:validFrom {validFrom} ; bsbm:validTo {validTo} ; bsbm:deliveryDays {deliveryDays} ; bsbm:offerWebpage {offerWebpage} ; dc:date {publishDate} ; bsbm:product bsbm-inst:dataFromProducer{producer}/Product{product} ; bsbm:vendor bsbm-inst:dataFromVendor{vendor}/Vendor{vendor} ; dc:publisher bsbm-inst:dataFromVendor{vendor}/Vendor{vendor} . 
source		select nr, product, producer, vendor, price, validFrom, validTo, deliveryDays, offerWebpage, publisher, publishDate from offer

mappingId	Person
target		bsbm-inst:dataFromRatingSite{publisher}/Reviewer{nr} a foaf:Person . 
source		select nr, name, mbox_sha1sum, country, publisher, publishDate from person

mappingId	Person2
target		bsbm-inst:dataFromRatingSite{publisher}/Reviewer{nr} foaf:name {name} ; foaf:mbox_sha1sum {mbox_sha1sum} ; bsbm:country iso3166:{country} . 
source		select nr, name, mbox_sha1sum, country, publisher, publishDate from person

mappingId	Person3
target		bsbm-inst:dataFromRatingSite{publisher}/Reviewer{nr} dc:publisher bsbm-inst:dataFromRatingSite{publisher}/RatingSite{publisher} . 
source		select nr, name, mbox_sha1sum, country, publisher, publishDate from person

mappingId	Person4
target		bsbm-inst:dataFromRatingSite{publisher}/Reviewer{nr} dc:date {publishDate} . 
source		select nr, name, mbox_sha1sum, country, publisher, publishDate from person

mappingId	Producer
target		bsbm-inst:dataFromProducer{nr}/Producer{nr} a bsbm:Producer . 
source		select nr, label, comment, homepage, country, publisher, publishDate from producer

mappingId	Producer2
target		bsbm-inst:dataFromProducer{nr}/Producer{nr} rdfs:label {label} ; rdfs:comment {comment} . 
source		select nr, label, comment, homepage, country, publisher, publishDate from producer

mappingId	Producer3
target		bsbm-inst:dataFromProducer{nr}/Producer{nr} foaf:homepage {homepage} . 
source		select nr, label, comment, homepage, country, publisher, publishDate from producer

mappingId	Producer4
target		bsbm-inst:dataFromProducer{nr}/Producer{nr} bsbm:country iso3166:{country} . 
source		select nr, label, comment, homepage, country, publisher, publishDate from producer

mappingId	Producer6
target		bsbm-inst:dataFromProducer{nr}/Producer{nr} dc:date {publishDate} . 
source		select nr, label, comment, homepage, country, publisher, publishDate from producer

mappingId	Product
target		bsbm-inst:dataFromProducer{producer}/Product{nr} a bsbm:Product . 
source		select nr, label, comment, producer, propertyNum1, propertyNum2, propertyNum3, propertyNum4, propertyNum5, propertyNum6, propertyTex1, propertyTex2, propertyTex3, propertyTex4, propertyTex5, publisher, propertyTex6, publishDate from product

mappingId	Product2
target		bsbm-inst:dataFromProducer{producer}/Product{nr} bsbm:productPropertyNumeric1 {propertyNum1} ; bsbm:productPropertyNumeric2 {propertyNum2} ; bsbm:productPropertyNumeric3 {propertyNum3} ; bsbm:productPropertyNumeric4 {propertyNum4} ; bsbm:productPropertyNumeric5 {propertyNum5} ; bsbm:productPropertyNumeric6 {propertyNum6} ; bsbm:productPropertyTextual1 {propertyTex1} ; bsbm:productPropertyTextual2 {propertyTex2} ; bsbm:productPropertyTextual3 {propertyTex3} ; bsbm:productPropertyTextual4 {propertyTex4} ; bsbm:productPropertyTextual5 {propertyTex5} ; bsbm:productPropertyTextual6 {propertyTex6} ; dc:date {publishDate} ; bsbm:producer bsbm-inst:dataFromProducer{producer}/Producer{producer} . 
source		select nr, label, comment, producer, propertyNum1, propertyNum2, propertyNum3, propertyNum4, propertyNum5, propertyNum6, propertyTex1, propertyTex2, propertyTex3, propertyTex4, propertyTex5, publisher, propertyTex6, publishDate from product

mappingId	Product3
target		bsbm-inst:dataFromProducer{producer}/Product{nr} dc:publisher bsbm-inst:dataFromProducer{producer}/Producer{producer} . 
source		select nr, label, comment, producer, propertyNum1, propertyNum2, propertyNum3, propertyNum4, propertyNum5, propertyNum6, propertyTex1, propertyTex2, propertyTex3, propertyTex4, propertyTex5, publisher, propertyTex6, publishDate from product

mappingId	Product4
target		bsbm-inst:dataFromProducer{producer}/Product{nr} rdfs:label {label} . 
source		select nr, label, comment, producer, propertyNum1, propertyNum2, propertyNum3, propertyNum4, propertyNum5, propertyNum6, propertyTex1, propertyTex2, propertyTex3, propertyTex4, propertyTex5, publisher, propertyTex6, publishDate from product

mappingId	Product5
target		bsbm-inst:dataFromProducer{producer}/Product{nr} rdfs:comment {comment} . 
source		select nr, label, comment, producer, propertyNum1, propertyNum2, propertyNum3, propertyNum4, propertyNum5, propertyNum6, propertyTex1, propertyTex2, propertyTex3, propertyTex4, propertyTex5, publisher, propertyTex6, publishDate from product

mappingId	Product has Product Feature
target		bsbm-inst:dataFromProducer{product_producer}/Product{product_nr} bsbm:productFeature bsbm-inst:ProductFeature{feature_product} . 
source		select product.producer as product_producer, product.nr as product_nr, productfeatureproduct.productFeature as feature_product from product, productfeatureproduct where product.nr = productfeatureproduct.product

mappingId	Product has Product Type
target		bsbm-inst:dataFromProducer{product_producer}/Product{product_nr} bsbm:type bsbm-inst:ProductType{productType} . 
source		select product.producer as product_producer, product.nr as product_nr, producttypeproduct.productType from product, producttypeproduct where product.nr = producttypeproduct.product

mappingId	Product Feature
target		bsbm-inst:ProductFeature{nr} a bsbm:ProductFeature . 
source		select nr, label, comment, publisher, publishDate from productfeature

mappingId	Product Feature2
target		bsbm-inst:ProductFeature{nr} rdfs:label {label} . 
source		select nr, label, comment, publisher, publishDate from productfeature

mappingId	Product Feature3
target		bsbm-inst:ProductFeature{nr} rdfs:comment {comment} . 
source		select nr, label, comment, publisher, publishDate from productfeature

mappingId	Product Feature4
target		bsbm-inst:ProductFeature{nr} dc:publisher bsbm-inst:StandardizationInstitution{publisher} . 
source		select nr, label, comment, publisher, publishDate from productfeature

mappingId	Product Feature5
target		bsbm-inst:ProductFeature{nr} dc:date {publishDate} . 
source		select nr, label, comment, publisher, publishDate from productfeature

mappingId	Product Type
target		bsbm-inst:ProductType{nr} a bsbm:ProductType . 
source		select nr, label, comment, parent, publisher, publishDate from producttype

mappingId	Product Type2
target		bsbm-inst:ProductType{nr} rdfs:label {label} ; rdfs:comment {comment} . 
source		select nr, label, comment, parent, publisher, publishDate from producttype

mappingId	Product Type3
target		bsbm-inst:ProductType{nr} dc:publisher bsbm-inst:StandardizationInstitution{publisher} . 
source		select nr, label, comment, parent, publisher, publishDate from producttype

mappingId	Product Type4
target		bsbm-inst:ProductType{nr} bsbm:subClassOf bsbm-inst:ProductType{parent} . 
source		select nr, label, comment, parent, publisher, publishDate from producttype

mappingId	Product Type5
target		bsbm-inst:ProductType{nr} dc:date {publishDate} . 
source		select nr, label, comment, parent, publisher, publishDate from producttype

mappingId	Review
target		bsbm-inst:dataFromRatingSite{publisher}/Review{nr} a bsbm:Review . 
source		select nr, product, producer, person, reviewDate, title, text, language, rating1, rating2, rating3, rating4, publisher, publishDate from review

mappingId	Review1
target		bsbm-inst:dataFromRatingSite{publisher}/Review{nr} bsbm:reviewDate {reviewDate} . 
source		select nr, product, producer, person, reviewDate, title, text, language, rating1, rating2, rating3, rating4, publisher, publishDate from review

mappingId	Review2
target		bsbm-inst:dataFromRatingSite{publisher}/Review{nr} dc:title {title} . 
source		select nr, product, producer, person, reviewDate, title, text, language, rating1, rating2, rating3, rating4, publisher, publishDate from review

mappingId	Review3
target		bsbm-inst:dataFromRatingSite{publisher}/Review{nr} bsbm:rating1 {rating1} ; bsbm:rating2 {rating2} ; bsbm:rating3 {rating3} ; bsbm:rating4 {rating4} . 
source		select nr, product, producer, person, reviewDate, title, text, language, rating1, rating2, rating3, rating4, publisher, publishDate from review

mappingId	Review4
target		bsbm-inst:dataFromRatingSite{publisher}/Review{nr} bsbm:reviewFor bsbm-inst:dataFromProducer{producer}/Product{product} . 
source		select nr, product, producer, person, reviewDate, title, text, language, rating1, rating2, rating3, rating4, publisher, publishDate from review

mappingId	Review5
target		bsbm-inst:dataFromRatingSite{publisher}/Review{nr} rev:reviewer bsbm-inst:dataFromRatingSite{publisher}/Reviewer{person} . 
source		select nr, product, producer, person, reviewDate, title, text, language, rating1, rating2, rating3, rating4, publisher, publishDate from review

mappingId	Review6
target		bsbm-inst:dataFromRatingSite{publisher}/Review{nr} dc:publisher bsbm-inst:dataFromRatingSite{publisher}/RatingSite{publisher} . 
source		select nr, product, producer, person, reviewDate, title, text, language, rating1, rating2, rating3, rating4, publisher, publishDate from review

mappingId	Review7
target		bsbm-inst:dataFromRatingSite{publisher}/Review{nr} rev:text {text}@{language} . 
source		select nr, product, producer, person, reviewDate, title, text, language, rating1, rating2, rating3, rating4, publisher, publishDate from review

mappingId	Review8
target		bsbm-inst:dataFromRatingSite{publisher}/Review{nr} dc:date {publishDate} . 
source		select nr, product, producer, person, reviewDate, title, text, language, rating1, rating2, rating3, rating4, publisher, publishDate from review

mappingId	Vendor
target		bsbm-inst:dataFromVendor{nr}/Vendor{nr} a bsbm:Vendor . 
source		select nr, label, comment, homepage, country, publisher, publishDate from vendor

mappingId	Vendor2
target		bsbm-inst:dataFromVendor{nr}/Vendor{nr} rdfs:label {label} ; rdfs:comment {comment} . 
source		select nr, label, comment, homepage, country, publisher, publishDate from vendor

mappingId	Vendor3
target		bsbm-inst:dataFromVendor{nr}/Vendor{nr} foaf:homepage {homepage} . 
source		select nr, label, comment, homepage, country, publisher, publishDate from vendor

mappingId	Vendor4
target		bsbm-inst:dataFromVendor{nr}/Vendor{nr} bsbm:country iso3166:{country} . 
source		select nr, label, comment, homepage, country, publisher, publishDate from vendor

mappingId	Vendor5
target		bsbm-inst:dataFromVendor{nr}/Vendor{nr} dc:publisher bsbm-inst:dataFromVendor{nr}/Vendor{nr} . 
source		select nr, label, comment, homepage, country, publisher, publishDate from vendor

mappingId	Vendor6
target		bsbm-inst:dataFromVendor{nr}/Vendor{nr} dc:date {publishDate} . 
source		select nr, label, comment, homepage, country, publisher, publishDate from vendor
]]

