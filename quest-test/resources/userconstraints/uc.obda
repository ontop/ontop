[SourceDeclaration]
sourceUri	oracleBolzano
connectionUrl	jdbc:h2:mem:countries
username	sa
password	
driverClass	org.h2.Driver

[MappingDeclaration] @collection [[
mappingId	hasVal1
target		:Class1-{COL1} :hasVal1 {COL1} . 
source		select "COL1" FROM "TABLE1"

mappingId	hasVal2
target		:Class1-{COL1} :hasVal2 {COL2} . 
source		select "COL1", "COL2" FROM "TABLE1"
]]

