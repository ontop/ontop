[PrefixDeclaration]
:		http://vocabulary.example.org/
xsd:		http://www.w3.org/2001/XMLSchema#

[MappingDeclaration] @collection [[
mappingId	person
target		:person/{id} a :Person ; :name {name} ; :birthDate {birth_date}^^xsd:date;  :birthInstant {birth_instant}^^xsd:dateTime; :age {age} .
source		SELECT id, age, name, birth_date, birth_instant FROM "person";
]]