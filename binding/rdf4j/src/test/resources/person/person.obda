[MappingDeclaration] @collection [[
mappingId	MAPID-person
target		:person/{"id"} a :Person ; :fullName {"fullName"} ; :country {"country"} ; :locality {"locality"} .
source		SELECT "id", "fullName", "status", "country", "locality" FROM "hr"."persons";

]]

