ans1(p, name, mbox_sha1sum, country, r, product, title)
CONSTRUCT [p, name, mbox_sha1sum, country, r, product, title] [p/RDF(http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/dataFromRatingSite/Reviewer{}(INTEGERToTEXT(person2m7)),IRI), country/RDF(http://downlode.org/rdf/iso-3166/countries#{}(CHARToTEXT(country3m1)),IRI), product/RDF(http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/dataFromProducer/Product{}(INTEGERToTEXT(product2m4)),IRI), r/RDF(http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/dataFromRatingSite/Review{}(INTEGERToTEXT(nr0m4)),IRI), name/RDF(VARCHARToTEXT(name1m12),xsd:string), mbox_sha1sum/RDF(CHARToTEXT(mbox_sha1sum1m41),xsd:string), title/RDF(VARCHARToTEXT(title2m11),xsd:string)]
   DISTINCT
      JOIN
         UNION [person2m7, name1m12, mbox_sha1sum1m41, country3m1]
            JOIN
               EXTENSIONAL "reviewc"(0:"1356"^^INTEGER,3:person2m7)
               EXTENSIONAL "person"(0:person2m7,1:name1m12,2:mbox_sha1sum1m41,3:country3m1)
            JOIN
               EXTENSIONAL "review"(0:"1356"^^INTEGER,3:person2m7)
               EXTENSIONAL "person"(0:person2m7,1:name1m12,2:mbox_sha1sum1m41,3:country3m1)
         UNION [nr0m4, person2m7]
            EXTENSIONAL "reviewc"(0:nr0m4,3:person2m7)
            EXTENSIONAL "review"(0:nr0m4,3:person2m7)
         UNION [nr0m4, product2m4]
            EXTENSIONAL "reviewc"(0:nr0m4,1:product2m4)
            EXTENSIONAL "review"(0:nr0m4,1:product2m4)
         UNION [nr0m4, title2m11]
            EXTENSIONAL "reviewc"(0:nr0m4,5:title2m11)
            EXTENSIONAL "review"(0:nr0m4,5:title2m11)
