   DISTINCT
      LJ
         LJ AND5(DATE_GT(validto1m39,"2008-06-13"^^DATE),IS_NOT_NULL(price1m34),IS_NOT_NULL(label10m4),IS_NOT_NULL(validto1m39),IS_NOT_NULL(nr1m5))
