    <owl:NamedIndividual rdf:about="http://example.org/101">
        <rdf:type rdf:resource="http://example.org/Geom"/>
        <ex:label> Second ontology point</ex:label>
        <geo:asWKT rdf:datatype="http://www.opengis.net/ont/geosparql#wktLiteral">POINT(11.777499931163218 46.950015945557745)</geo:asWKT>
    </owl:NamedIndividual>

