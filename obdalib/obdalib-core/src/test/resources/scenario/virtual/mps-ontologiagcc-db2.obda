    <SavedQueries>
        <QueryGroup id="Query Campione ">
            <Query id="QUERY 1" text="/*QUERY 1 Per un determinato NGR, esporre i dati (NGR1, NGR2, Tipo legame, Timestamp di inizio validit) di tutti i legami (dati grezzi) cui esso partecipa (come collegante o come collegato) ad un certo Timestamp di riferimento. */ SELECT 'ngr1', cliente.ngr2, cliente.ti, cliente.tl FROM sparqltable( select ?ngr2 ?ti ?tf ?tl where { &#x9;?x rdf:type 'Cliente'. &#x9;?x :NGR 'ngr1'. &#x9;?y rdf:type 'Cliente'. &#x9;?y :NGR ?ngr2. &#x9;?x :eCollegato ?l. &#x9;?y :eCollegante ?l. &#x9;?l :tipoLegame ?tl. &#x9;?l :timestampI ?ti. &#x9;?I :timestampF ?tf } ) cliente WHERE cliente.tf > '06/07/2008' UNION SELECT 'ngr1',cliente.ngr2, cliente.ti, cliente.tl FROM sparqltable( select ?ngr2 ?ti ?tf ?tl where { &#x9;?x rdf:type 'Cliente'. &#x9;?x :NGR 'ngr1'. &#x9;?y rdf:type 'Cliente'. &#x9;?y :NGR ?ngr2. &#x9;?y :eCollegato ?l. &#x9;?x :eCollegante ?l. &#x9;?l :tipoLegame ?tl. &#x9;?l :timestampI ?ti. &#x9;?I :timestampF ?tf } ) cliente WHERE cliente.tf > '06/07/2008' "/>
            <Query id="QUERY 2" text="/*QUERY 2 Estrarre l&#x2019;elenco delle entit appartenenti a GCC giuridici &quot;In Monitoraggio&quot;, evidenziando, ad un certo Timestamp, i seguenti dati: NGR, ID_PERIMETRO, DENOMINAZIONE DEL GRUPPO AZIENDALE relativo al perimetro, FLAG CAPOGRUPPO S/N. */ SELECT temp.ngr, temp.igcc, temp.nomeG, 'TRUE' FROM sparqltable( &#x9;select ?ngr ?igcc ?nomeG ?c ?H &#x9;where { &#x9;&#x9;?x rdf:type 'GCCGiuridico'. &#x9;&#x9;?x :eSottoMonitoraggio ?pv. &#x9;&#x9;?pv :timestampI 't1'. &#x9;&#x9;?x :IGCC ?igcc. &#x9;&#x9;?per :inGCC ?x. &#x9;&#x9;?per :haCliente ?c. &#x9;&#x9;?c :NGR ?ngr. &#x9;&#x9;?perH :inGCC ?x. &#x9;&#x9;?perH :haCliente ?H. &#x9;&#x9;?H :inAppartenenza ?app. &#x9;&#x9;?app rdf:type 'Holding'. &#x9;&#x9;?G :haAppartenenza ?app. &#x9;&#x9;?G :haDenominazione ?d. &#x9;&#x9;?d :stringaDenominazione ?nomeG } &#x9;) temp WHERE temp.c = temp.H UNION SELECT temp.ngr, temp.igcc, temp.nomeG, 'FALSE' FROM sparqltable( &#x9;select ?ngr ?igcc ?nomeG ?c ?H &#x9;where { &#x9;&#x9;?x rdf:type 'GCCGiuridico'. &#x9;&#x9;?x :eSottoMonitoraggio ?pv. &#x9;&#x9;?pv :timestampI 't1'. &#x9;&#x9;?x :IGCC ?igcc. &#x9;&#x9;?per :inGCC ?x. &#x9;&#x9;?per :haCliente ?c. &#x9;&#x9;?c :NGR ?ngr. &#x9;&#x9;?perH :inGCC ?x. &#x9;&#x9;?perH :haCliente ?H. &#x9;&#x9;?H :inAppartenenza ?app. &#x9;&#x9;?app rdf:type 'Holding'. &#x9;&#x9;?G :haAppartenenza ?app. &#x9;&#x9;?G :haDenominazione ?d. &#x9;&#x9;?d :stringaDenominazione ?nomeG } &#x9;) temp WHERE temp.c > temp.H "/>
        </QueryGroup>
        <QueryGroup id="Query epistemiche valutate">
            <Query id="Clienti_non_fittizi" text="/* i Clienti non fittizi */ select cliente.x &#x9;from&#x9;sparqltable( &#x9;&#x9;&#x9;select ?x &#x9;&#x9;&#x9;where{ ?x rdf:type 'Cliente' } &#x9;&#x9;                    ) cliente &#x9;where cliente.x not in ( &#x9;&#x9;          select clienteFit.x  &#x9;&#x9;          from&#x9;sparqltable( &#x9;&#x9;&#x9;&#x9;select ?x &#x9;&#x9;&#x9;&#x9;where{ ?x rdf:type 'ClienteFittizio' } &#x9;&#x9;&#x9;                      ) clienteFit &#x9;&#x9; &#x9;&#x9;          ) "/>
        </QueryGroup>
        <QueryGroup id="Query Valutate">
            <Query id="clienti-evento perimetrazione" text="#Coppie di clienti associati al medesimo GCC da un evento di perimetrazione  select ?c1 ?c2 where{ &#x9;&#x9;?c1 rdf:type 'Cliente'. &#x9;&#x9;?c2 rdf:type 'Cliente'. &#x9;&#x9;?c1 :inEventoPerimetrazione ?e1. &#x9;&#x9;?c2 :inEventoPerimetrazione ?e2. &#x9;&#x9;?e1 rdf:type 'Evento'. &#x9;&#x9;?e2 rdf:type 'Evento'. &#x9;&#x9;?e1 :GCCRelativo ?g. &#x9;&#x9;?e2 :GCCRelativo ?g&#x9;&#x9; &#x9;} "/>
            <Query id="clienti-legame economico" text="# Coppie di clienti legate da un legame di tipo economico&quot; select ?c1 ?c2 where{&#x9;&#x9;?c1 rdf:type 'Cliente'. &#x9;&#x9;?c2 rdf:type 'Cliente'. &#x9;&#x9;?c1 :eCollegato ?l. &#x9;&#x9;?c2 :eCollegante ?l. &#x9;&#x9;?l rdf:type 'LegameTpEconomico' . &#x9;}   "/>
            <Query id="cliente_specifico-gcc" text="#Calcolare tutti tutti i GCC a cui appartiene il cliente con NGR = 25 SELECT $gcc   WHERE { &#x9;&#x9;    $cl :NGR '25^^xsd:integer'. &#x9;&#x9;    $per :haCliente $cl. &#x9;&#x9;    $per :inGCC $gcc. &#x9;&#x9;   }"/>
            <Query id="gcc_specifico-clienti" text="#Calcolare tutti i clienti che appartengono al perimetro con IGA = 1 e timestampI = 2006-09-27 23:59:59.0 &#x9;SELECT $cl   WHERE { &#x9;&#x9;    $gcc :IGCC '1^^xsd:integer'. &#x9;&#x9;    $gcc :timestampI '2006-09-27 23:59:59.0^^xsd:dateTime'. &#x9;&#x9;    $per :inGCC $gcc.&#x9; &#x9;&#x9;    $per :haCliente $cl&#x9; &#x9;&#x9;   }"/>
            <Query id="cliente_specifico-nome Gruppo Aziendale" text="#Calcolare il nome del gruppo aziendale a cui appartiene il cliente con ngr = 7611 SELECT $nome   WHERE { &#x9;&#x9;    $cl :NGR '7611^^xsd:integer'. &#x9;&#x9;    $cl :inAppartenenza $app. &#x9;&#x9;    $ga :haAppartenenza $app. &#x9;&#x9;    $ga :haDenominazione $den. &#x9;&#x9;    $den :stringaDenominazione $nome&#x9;&#x9; &#x9;&#x9;   }"/>
            <Query id="clienteFittizio_specifico-clienti" text="#Calcolare i clienti che compongono il ClienteFittizio con NGR = -24805 SELECT $cl   WHERE { &#x9;&#x9;    $cf rdf:type 'ClienteFittizio'. &#x9;&#x9;    $cf :NGR '-24805^^xsd:integer'. &#x9;&#x9;   $cf :haComposizione $comp. &#x9;&#x9;   $cl :inComposizione $comp.&#x9; &#x9;&#x9;   }"/>
            <Query id="gcc_sotto_reportistica" text="#Calcolare l'IGCC di tutti i GCC che sono sotto Reportistica SELECT $igcc WHERE { &#x9;&#x9;    $gcc :eSottoReportistica $pv. &#x9;&#x9;    $gcc :IGCC $igcc&#x9;&#x9;&#x9; &#x9;&#x9;   }"/>
            <Query id="igcc_dei_GCCGiuridici" text="#Calcolare l'IGCC di tutti i GCC Giuridici SELECT $igcc WHERE { &#x9;&#x9;    $gcc rdf:type 'GCCGiuridico'. &#x9;&#x9;    $gcc :IGCC $igcc&#x9;&#x9;&#x9; &#x9;&#x9;   }"/>
            <Query id="Clienti_Capogruppo" text="#Capogruppo select ?c where{ &#x9;?c rdf:type 'Cliente'. &#x9;?c :inAppartenenza ?h. &#x9;?h rdf:type 'Holding'&#x9; }  "/>
            <Query id="ngr-importoFatturato" text="# restituire importo fatturato ed ngr di ongi cliente SELECT $ngr $imp WHERE { $cl :NGR $ngr. &#x9;&#x9;$cl :haDatiBilancio $db. &#x9;&#x9;$db  :importoFatturato $imp }"/>
            <Query id="gcc_specifico-ngr-clienti" text="#Calcolare l'NGR di tutti i clienti che appartengono al perimetro con IGCC = 1 e timestampI = 2006-09-27 23:59:59.0 &#x9;SELECT $ngr   WHERE { &#x9;&#x9;    $gcc :IGCC '1^^xsd:integer'. &#x9;&#x9;    $gcc :timestampI '2006-09-27 23:59:59.0^^xsd:dateTime'. &#x9;&#x9;    $per :inGCC $gcc.&#x9; &#x9;&#x9;    $per :haCliente $cl. &#x9;&#x9;    $cl :NGR $ngr&#x9; &#x9;&#x9;   }"/>
            <Query id="cliente-ga-legame_economico" text="# Coppie di clienti legate da un legame di tipo economico ed appartenenti al medesimo gruppo aziendale - nota ogni disgiunto della query unfoldata  valutabile sul DB, ma non  eseguita dal sistema  select ?x ?y&#x9; where{ &#x9;&#x9;?x rdf:type 'Cliente'. &#x9;&#x9;?x :eCollegato ?l. &#x9;&#x9;?l rdf:type 'LegameTpEconomico' . &#x9;&#x9;?y :eCollegante ?l. &#x9;&#x9;?x :inAppartenenza ?a1. &#x9;&#x9;?y :inAppartenenza ?a2. &#x9;&#x9;?g :haAppartenenza ?a1. &#x9;&#x9;?g :haAppartenenza ?a2 &#x9;}  "/>
            <Query id="NGR componenti di cliente fittizio" text="#Calcolare l'NGR dei clienti che compongono il ClienteFittizio con NGR = -24805 SELECT $ngr  WHERE { &#x9;&#x9;    $cf rdf:type 'ClienteFittizio'. &#x9;&#x9;    $cf :NGR '-24805^^xsd:int'. &#x9;&#x9;   $cf :haComposizione $comp. &#x9;&#x9;   $cl :inComposizione $comp. &#x9;&#x9;   $cl :NGR $ngr&#x9;&#x9; &#x9;&#x9;   }"/>
        </QueryGroup>
        <QueryGroup id="Query non valutate">
            <Query id="cliente_specifico-igcc-timestampI_perimetro" text="#Calcolare tutti IGCC e timestampI di tutti i perimetri a cui appartiene il cliente con NGR = 25 SELECT $igcc $ti   WHERE { &#x9;&#x9;    $cl :NGR '25^^xsd:integer'. &#x9;&#x9;    $per :haCliente $cl. &#x9;&#x9;    $per :inGCC $gcc. &#x9;&#x9;    $gcc :timestampI $ti. &#x9;&#x9;    $gcc :IGCC $igcc&#x9; &#x9;&#x9;   }"/>
            <Query id="clienti-legame-gcc" text="# Coppie di clienti legate da un legame di tipo economico ed appartenenti al medesimo GCC in modo &quot;Effettivo&quot; select ?x ?y where{ &#x9;&#x9;?x rdf:type 'Cliente'. &#x9;&#x9;?x :eCollegato ?l. &#x9;&#x9;?l rdf:type 'LegameTpEconomico' . &#x9;&#x9;?y :eCollegante ?l. &#x9;&#x9;?p1 :haCliente ?x. &#x9;&#x9;?p2 :haCliente ?y. &#x9;&#x9;?p1 :inGCC ?g. &#x9;&#x9;?p2 :inGCC ?g. &#x9;&#x9;?p1 rdf:type 'PerimetrazioneEffettiva'. &#x9;&#x9;?p2 rdf:type 'PerimetrazioneEffettiva'. &#x9;}   "/>
            <Query id="Corrispondenza_gcc-ga" text="# Coppie di clienti legate da un legame di tipo economico ed appartenenti al medesimo GCC in modo &quot;Effettivo&quot; select ?x ?y where{ &#x9;&#x9;?x rdf:type 'Cliente'. &#x9;&#x9;?x :eCollegato ?l. &#x9;&#x9;?l rdf:type 'LegameTpEconomico' . &#x9;&#x9;?y :eCollegante ?l. &#x9;&#x9;?p1 :haCliente ?x. &#x9;&#x9;?p2 :haCliente ?y. &#x9;&#x9;?p1 :inGCC ?g. &#x9;&#x9;?p2 :inGCC ?g. &#x9;&#x9;?p1 rdf:type 'PerimetrazioneEffettiva'. &#x9;&#x9;?p2 rdf:type 'PerimetrazioneEffettiva'. &#x9;}   "/>
        </QueryGroup>
        <QueryGroup id="">
