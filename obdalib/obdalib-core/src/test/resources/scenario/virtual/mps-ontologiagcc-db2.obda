    <QueryGroup id="Query Campione ">
      <Query id="QUERY 1" text="/*QUERY 1 Per un determinato NGR, esporre i dati (NGR1, NGR2, Tipo legame, Timestamp di inizio validit) di tutti i legami (dati grezzi) cui esso partecipa (come collegante o come collegato) ad un certo Timestamp di riferimento. */ SELECT 'ngr1', cliente.ngr2, cliente.ti, cliente.tl FROM sparqltable( select ?ngr2 ?ti ?tf ?tl where { &#9;?x rdf:type 'Cliente'. &#9;?x :NGR 'ngr1'. &#9;?y rdf:type 'Cliente'. &#9;?y :NGR ?ngr2. &#9;?x :eCollegato ?l. &#9;?y :eCollegante ?l. &#9;?l :tipoLegame ?tl. &#9;?l :timestampI ?ti. &#9;?I :timestampF ?tf } ) cliente WHERE cliente.tf &gt; '06/07/2008' UNION SELECT 'ngr1',cliente.ngr2, cliente.ti, cliente.tl FROM sparqltable( select ?ngr2 ?ti ?tf ?tl where { &#9;?x rdf:type 'Cliente'. &#9;?x :NGR 'ngr1'. &#9;?y rdf:type 'Cliente'. &#9;?y :NGR ?ngr2. &#9;?y :eCollegato ?l. &#9;?x :eCollegante ?l. &#9;?l :tipoLegame ?tl. &#9;?l :timestampI ?ti. &#9;?I :timestampF ?tf } ) cliente WHERE cliente.tf &gt; '06/07/2008' "/>
      <Query id="QUERY 2" text="/*QUERY 2 Estrarre l&#8217;elenco delle entit appartenenti a GCC giuridici &quot;In Monitoraggio&quot;, evidenziando, ad un certo Timestamp, i seguenti dati: NGR, ID_PERIMETRO, DENOMINAZIONE DEL GRUPPO AZIENDALE relativo al perimetro, FLAG CAPOGRUPPO S/N. */ SELECT temp.ngr, temp.igcc, temp.nomeG, 'TRUE' FROM sparqltable( &#9;select ?ngr ?igcc ?nomeG ?c ?H &#9;where { &#9;&#9;?x rdf:type 'GCCGiuridico'. &#9;&#9;?x :eSottoMonitoraggio ?pv. &#9;&#9;?pv :timestampI 't1'. &#9;&#9;?x :IGCC ?igcc. &#9;&#9;?per :inGCC ?x. &#9;&#9;?per :haCliente ?c. &#9;&#9;?c :NGR ?ngr. &#9;&#9;?perH :inGCC ?x. &#9;&#9;?perH :haCliente ?H. &#9;&#9;?H :inAppartenenza ?app. &#9;&#9;?app rdf:type 'Holding'. &#9;&#9;?G :haAppartenenza ?app. &#9;&#9;?G :haDenominazione ?d. &#9;&#9;?d :stringaDenominazione ?nomeG } &#9;) temp WHERE temp.c = temp.H UNION SELECT temp.ngr, temp.igcc, temp.nomeG, 'FALSE' FROM sparqltable( &#9;select ?ngr ?igcc ?nomeG ?c ?H &#9;where { &#9;&#9;?x rdf:type 'GCCGiuridico'. &#9;&#9;?x :eSottoMonitoraggio ?pv. &#9;&#9;?pv :timestampI 't1'. &#9;&#9;?x :IGCC ?igcc. &#9;&#9;?per :inGCC ?x. &#9;&#9;?per :haCliente ?c. &#9;&#9;?c :NGR ?ngr. &#9;&#9;?perH :inGCC ?x. &#9;&#9;?perH :haCliente ?H. &#9;&#9;?H :inAppartenenza ?app. &#9;&#9;?app rdf:type 'Holding'. &#9;&#9;?G :haAppartenenza ?app. &#9;&#9;?G :haDenominazione ?d. &#9;&#9;?d :stringaDenominazione ?nomeG } &#9;) temp WHERE temp.c &gt; temp.H "/>
    </QueryGroup>
