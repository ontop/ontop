<>  rdf:type mf:Manifest ;
    rdfs:label "Stockexchange Datatype Queries" ;
    mf:entries
    ( 
      :literal-1 :literal-2
      :string-1 :string-2 :string-3
      :integer-1 :integer-2 :integer-3
      :pos-integer-1 :pos-integer-2 :pos-integer-3
      :neg-integer-1 :neg-integer-2 :neg-integer-3
      :decimal-1 :decimal-2 :decimal-3
      :pos-decimal-1 :pos-decimal-2 :pos-decimal-3
      :neg-decimal-1 :neg-decimal-2 :neg-decimal-3
      :double-1 :double-2 :double-3						## Double test Queries were adjusted for DB2
      :pos-double-1 :pos-double-2 :pos-double-3      	## There are rounding issues present in DB2 with comparing EQ on doubles  
      :neg-double-1 :neg-double-2 :neg-double-3
      :datetime-1a :datetime-1b :datetime-1c
      :datetime-2a :datetime-2b :datetime-2c
      :datetime-3a :datetime-3b :datetime-3c :datetime-3d # in UTC
      #:datetime-3e # ERROR: in UTC with minus offset
      :datetime-3f :datetime-3g
      #:datetime-3h # ERROR: in UTC with plus offset
      :datetime-3i :datetime-3j
      :boolean-1a :boolean-1b :boolean-1c :boolean-1d	## DB2 doesnt have boolean data type, therefore we store 0/1 
      :boolean-2a :boolean-2b :boolean-2c :boolean-2d	## and change tests and mappings accordingly
      :boolean-3a :boolean-3b :boolean-3c :boolean-3d	
    ) .

