@prefix beo:    <https://eia-demo.metaphacts.cloud/> .
@prefix common: <https://enterprise.semanticlayer.net/Ontology/Common/> .
@prefix data:   <http://mydata.example.org/> .
@prefix dc:     <http://purl.org/dc/elements/1.1/> .
@prefix dct:    <http://purl.org/dc/terms/> .
@prefix eslo:   <https://eia-demo.metaphacts.cloud/ontology/> .
@prefix geo:    <http://www.opengis.net/ont/geosparql#> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh:     <http://www.w3.org/ns/shacl#> .
@prefix skos:   <http://www.w3.org/2004/02/skos/core#> .
@prefix voc:    <http://vocabulary.example.org/> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .

eslo:ItSystemCategory
        rdf:type      rdfs:Class;
        rdfs:comment  "IT System Categories defined by Gartner"@en;
        rdfs:label    "IT System Category"@en .

eslo:worksAtDepartment
        rdf:type      owl:ObjectProperty;
        rdfs:comment  "Establishes a relationship between an Employee and the Department or Business Unit where they perform their professional duties. This relationship helps define reporting structures and organizational responsibilities."@en;
        rdfs:label    "works at department"@en .

rdfs:Literal  rdf:type   rdfs:Class;
        rdfs:comment     "The class of literal values, eg. textual strings and integers.";
        rdfs:label       "Literal";
        rdfs:subClassOf  rdfs:Resource .

rdfs:seeAlso  rdf:type  rdf:Property;
        rdfs:comment  "Further information about the subject resource.";
        rdfs:domain   rdfs:Resource;
        rdfs:label    "seeAlso";
        rdfs:range    rdfs:Resource .

rdfs:member  rdf:type  rdf:Property;
        rdfs:comment  "A member of the subject resource.";
        rdfs:domain   rdfs:Resource;
        rdfs:label    "member";
        rdfs:range    rdfs:Resource .

eslo:isInputForBusinessProcess
        rdf:type      owl:ObjectProperty;
        rdfs:comment  "relationship links a business object as an input of a business process."@en;
        rdfs:label    "is input for business process"@en .

eslo:hasSubProcess  rdf:type  owl:ObjectProperty;
        rdfs:label  "has sub process"@en .

eslo:BusinessObject  rdf:type  rdfs:Class;
        rdfs:comment  "A Business Object represents a key concept or entity in an organization. Business Objects serve as core elements in business processes, enabling data integration and governance."@en;
        rdfs:label    "Business Object"@en .

rdfs:Datatype  rdf:type  rdfs:Class;
        rdfs:comment     "The class of RDF datatypes.";
        rdfs:label       "Datatype";
        rdfs:subClassOf  rdfs:Class .

eslo:hasItSystem  rdf:type  owl:ObjectProperty;
        rdfs:comment  "Connects an enterprise entity (e.g., a Business Object or an Business Process) to an IT System that supports its operations."@en;
        rdfs:label    "has it system"@en .

rdfs:Class  rdf:type     rdfs:Class;
        rdfs:comment     "The class of classes.";
        rdfs:label       "Class";
        rdfs:subClassOf  rdfs:Resource .

eslo:hasAccountableEmployee
        rdf:type      owl:ObjectProperty;
        rdfs:comment  "Person accountable for business process"@en;
        rdfs:label    "has accountable employee"@en .

eslo:hasResponsibleEmployee
        rdf:type      owl:ObjectProperty;
        rdfs:comment  "Person responsible for business process"@en;
        rdfs:label    "has responsible employee"@en .

common:version  rdf:type  rdf:Property .

eslo:interfacesWith  rdf:type  owl:ObjectProperty;
        rdfs:label  "interfaces with"@en .

rdfs:Resource  rdf:type  rdfs:Class;
        rdfs:comment  "The class resource, everything.";
        rdfs:label    "Resource" .

eslo:mapsTo  rdf:type  owl:ObjectProperty;
        rdfs:label  "maps to"@en .

rdfs:subClassOf  rdf:type  rdf:Property;
        rdfs:comment  "The subject is a subclass of a class.";
        rdfs:domain   rdfs:Class;
        rdfs:label    "subClassOf";
        rdfs:range    rdfs:Class .

eslo:isRelatedTo  rdf:type  owl:ObjectProperty;
        rdfs:label  "is related to"@en .

eslo:hasRole  rdf:type  owl:ObjectProperty;
        rdfs:comment  "Links an entity (such as an Employee) to a specific Role they fulfill in the enterprise. This relationship helps define responsibilities."@en;
        rdfs:label    "has role"@en .

eslo:hasItSystemCategory
        rdf:type      owl:ObjectProperty;
        rdfs:comment  "Links an IT System to IT System Categories"@en;
        rdfs:label    "has it system category"@en .

eslo:DataSchema  rdf:type  rdfs:Class;
        rdfs:label  "Data Schema"@en .

rdfs:label  rdf:type  rdf:Property;
        rdfs:comment  "A human-readable name for the subject.";
        rdfs:domain   rdfs:Resource;
        rdfs:label    "label" .

eslo:hasResponsibleDataSteward
        rdf:type      owl:ObjectProperty;
        rdfs:comment  "Links a Business Object to the Employee with the Role \"Data Steward\" responsible for data stewardship, ensuring data quality, integrity, and compliance."@en;
        rdfs:label    "has responsible data steward"@en .

eslo:hasDataSchema  rdf:type  owl:ObjectProperty;
        rdfs:label  "has data schema"@en .

eslo:Employee  rdf:type  rdfs:Class;
        rdfs:comment  "An Employee represents a person working within the organization, holding specific responsibilities, roles, and relationships to business processes, IT systems, and data governance."@en;
        rdfs:label    "Employee"@en .

rdfs:Container  rdf:type  rdfs:Class;
        rdfs:comment     "The class of RDF containers.";
        rdfs:label       "Container";
        rdfs:subClassOf  rdfs:Resource .

eslo:Role  rdf:type   rdfs:Class;
        rdfs:comment  "A Role represents a function or position held by an Employee within an enterprise context. Roles define responsibilities, access levels, and decision-making authority in enterprise processes."@en;
        rdfs:label    "Role"@en .

eslo:hasResponsibleInformationArchitect
        rdf:type      owl:ObjectProperty;
        rdfs:comment  "Links a Business Object to the Employee with the Role \"Information Architect\" responsible for designing its structure, relationships, and integration within the enterprise data ecosystem"@en;
        rdfs:label    "has responsible information architect"@en .

rdfs:isDefinedBy  rdf:type  rdf:Property;
        rdfs:comment        "The defininition of the subject resource.";
        rdfs:domain         rdfs:Resource;
        rdfs:label          "isDefinedBy";
        rdfs:range          rdfs:Resource;
        rdfs:subPropertyOf  rdfs:seeAlso .

eslo:alias  rdf:type  owl:DatatypeProperty;
        rdfs:label  "alias"@en .

rdfs:range  rdf:type  rdf:Property;
        rdfs:comment  "A range of the subject property.";
        rdfs:domain   rdf:Property;
        rdfs:label    "range";
        rdfs:range    rdfs:Class .

eslo:hasBusinessOwner
        rdf:type    owl:ObjectProperty;
        rdfs:label  "has business owner"@en .

eslo:isOutputForBusiness
        rdf:type      owl:ObjectProperty;
        rdfs:comment  "relationship links a business object as an output of a business process."@en;
        rdfs:label    "is output for business "@en .

eslo:isModelledBy  rdf:type  owl:ObjectProperty;
        rdfs:label  "is modelled by"@en .

rdfs:ContainerMembershipProperty
        rdf:type         rdfs:Class;
        rdfs:comment     "The class of container membership properties, rdf:_1, rdf:_2, ...,\n                    all of which are sub-properties of 'member'.";
        rdfs:label       "ContainerMembershipProperty";
        rdfs:subClassOf  rdf:Property .

eslo:description  rdf:type  owl:DatatypeProperty;
        rdfs:comment  "A human-readable textual summary that provides meaningful context about an entity within the ontology. This attribute captures essential details, including the purpose, characteristics, or function of the entity it describes. The description should be clear, concise, and informative, facilitating understanding by both humans and AI models."@en;
        rdfs:label    "description"@en .

owl:Class  rdf:type  rdfs:Class;
        rdfs:label  "Class"@en .

rdfs:subPropertyOf  rdf:type  rdf:Property;
        rdfs:comment  "The subject is a subproperty of a property.";
        rdfs:domain   rdf:Property;
        rdfs:label    "subPropertyOf";
        rdfs:range    rdf:Property .

eslo:emailAdress  rdf:type  owl:DatatypeProperty;
        rdfs:label  "email adress"@en .

eslo:hasDataProduct  rdf:type  owl:ObjectProperty;
        rdfs:comment  "Establishes a relationship between an entity (such as a Business Object, Business Process, or IT System) and a Data Product that it utilizes, produces, or governs. This relationship helps trace data ownership, lineage, and consumption within the enterprise."@en;
        rdfs:label    "has data product"@en .

eslo:ItSystem  rdf:type  rdfs:Class;
        rdfs:comment  "An IT System is a software platform or tool that supports enterprise operations, data management, and automation"@en;
        rdfs:label    "IT System"@en .

owl:Ontology  rdf:type  rdfs:Class;
        rdfs:label  "Ontology"@en .

eslo:Department  rdf:type  rdfs:Class;
        rdfs:comment  "A Department is a functional unit within an organization, grouping employees and resources to fulfill specific business operations. Departments play a key role in enterprise governance."@en;
        rdfs:label    "Department"@en .

eslo:hasTechnicalOwner
        rdf:type    owl:ObjectProperty;
        rdfs:label  "has technical owner"@en .

rdfs:comment  rdf:type  rdf:Property;
        rdfs:comment  "A description of the subject resource.";
        rdfs:domain   rdfs:Resource;
        rdfs:label    "comment" .

eslo:DataProduct  rdf:type  rdfs:Class;
        rdfs:comment  "A Data Product is a curated set of data assets that provide value to business users, analytics teams, or external consumers. It encapsulates datasets, metadata, governance rules, and access mechanisms to facilitate self-service data consumption"@en;
        rdfs:label    "Data Product"@en .

rdfs:isDefinedB  rdf:type  owl:ObjectProperty;
        rdfs:label  "isDefinedBy"@en .

eslo:BusinessProcess  rdf:type  rdfs:Class;
        rdfs:comment  "A Business Process represents a structured set of activities or workflows within an organization that transforms inputs into valuable outputs. These processes define business operations, governance, and decision-making across departments.\n"@en;
        rdfs:label    "Business Process"@en .

rdfs:domain  rdf:type  rdf:Property;
        rdfs:comment  "A domain of the subject property.";
        rdfs:domain   rdf:Property;
        rdfs:label    "domain";
        rdfs:range    rdfs:Class .
