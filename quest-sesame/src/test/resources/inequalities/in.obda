[PrefixDeclaration]
:		http://www.semanticweb.org/ontologies/2015/5/untitled-ontology-1636#

[SourceDeclaration]
sourceUri	oracleBolzano
connectionUrl	jdbc:h2:mem:blah
username	sa
password	
driverClass	org.h2.Driver

[MappingDeclaration] @collection [[
mappingId	Lt1
target		:obj-{COL1} a :Lt1 . 
source		SELECT COL1 FROM "TABLE" WHERE COL1 < 1

mappingId	Gt5
target		:obj-{COL1} a :Gt5 . 
source		SELECT COL1 FROM "TABLE" WHERE COL1 > 5

mappingId	Gt3
target		:obj-{COL1} a :Gt3 . 
source		SELECT COL1 FROM "TABLE" WHERE COL1 > 3

mappingId	Gt
target		:obj-{COL1} :Gt :obj-{COL2} . 
source		SELECT COL1, COL2 FROM "TABLE" WHERE COL1 > COL2

mappingId	Lt
target		:obj-{COL1} :Lt :obj-{COL2} . 
source		SELECT COL1, COL2 FROM "TABLE" WHERE COL1 < COL2

mappingId	Lte
target		:obj-{COL1} :Lte :obj-{COL2} . 
source		SELECT COL1, COL2 FROM "TABLE" WHERE COL1 <= COL2

mappingId	Neq
target		:obj-{COL1} :Neq :obj-{COL2} . 
source		SELECT COL1, COL2 FROM "TABLE" WHERE COL1 != COL2

mappingId	Eq
target		:obj-{COL1} :Eq :obj-{COL2} . 
source		SELECT COL1, COL2 FROM "TABLE" WHERE COL1 = COL2
]]
