[PrefixDeclaration]
:		http://www.semanticweb.org/ontologies/2015/5/untitled-ontology-1636#

[SourceDeclaration]
sourceUri	oracleBolzano
connectionUrl	jdbc:h2:mem:test
username	sa
password	
driverClass	org.h2.Driver

[MappingDeclaration] @collection [[
mappingId	Lt1
target		:obj{COL} a :Lt1 . :obj{COL} a :Lt1or2 .
source		SELECT COL FROM "TABLE" WHERE COL < 1

mappingId	Lt2
target		:obj{COL} a :Lt2 . :obj{COL} a :Lt1or2 .
source		SELECT COL FROM "TABLE" WHERE COL < 2

mappingId	Gt5
target		:obj{COL} a :Gt5 . :obj{COL} a :Gt3or5 .
source		SELECT COL FROM "TABLE" WHERE COL > 5

mappingId	Gt3
target		:obj{COL} a :Gt3 . :obj{COL} a :Gt3or5 .
source		SELECT COL FROM "TABLE" WHERE COL > 3

mappingId	Gt
target		:obj{COL1} :Gt :obj{COL2} .
source		SELECT T1.COL AS COL1, T2.COL AS COL2 FROM "TABLE" T1, "TABLE" T2 WHERE T1.COL > T2.COL

mappingId	Lt
target		:obj{COL1} :Lt :obj{COL2} .
source		SELECT T1.COL AS COL1, T2.COL AS COL2 FROM "TABLE" T1, "TABLE" T2 WHERE T1.COL < T2.COL

mappingId	Lte
target		:obj{COL1} :Lte :obj{COL2} .
source		SELECT T1.COL AS COL1, T2.COL AS COL2 FROM "TABLE" T1, "TABLE" T2 WHERE T1.COL <= T2.COL

mappingId	Neq
target		:obj{COL1} :Neq :obj{COL2} .
source		SELECT T1.COL AS COL1, T2.COL AS COL2 FROM "TABLE" T1, "TABLE" T2 WHERE T1.COL != T2.COL

mappingId	Eq
target		:obj{COL1} :Eq :obj{COL2} .
source		SELECT T1.COL AS COL1, T2.COL AS COL2 FROM "TABLE" T1, "TABLE" T2 WHERE T1.COL = T2.COL
]]
